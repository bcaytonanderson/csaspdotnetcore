<!DOCTYPE html>
<html>

	<head>
	<meta charset="utf-8">

	</head>


	<body>

		@model List<CharacterSheetApp.Models.Character>
		<h2>Characters</h2>

		<form asp-action="Create">
			<div>
				<input name="CharacterName" />
				<input type="submit" value="Add Character" />
			</div>
		</form>

		<div>
			<ul>

				@foreach (var item in Model)
				{
					<li>
						<label>@item.Name</label>
					</li>
				}
				
			</ul>
		</div>

	</body>

</html>